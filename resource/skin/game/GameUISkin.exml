		</e:Group>
		<e:layout>
			<e:HorizontalLayout gap="40" verticalAlign="middle"/>
		</e:layout>
	</e:Group>
	<e:Button id="defBtn" label="配置防守蛊虫" skinName="Btn1Skin" horizontalCenter="0" bottom="180" width="220"/>
