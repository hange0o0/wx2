			<e:Label text="养蛊" size="24" horizontalCenter="0" bottom="0" bold="true"/>
		</e:Group>
		<e:Group id="workBtn" x="0" y="0" width="110" height="140">
			<e:Image y="0" horizontalCenter="0" source="main_icon2_png"/>
